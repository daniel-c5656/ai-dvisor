{
  "hosting": {
    "public": "frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/run",
        "run": {
          "serviceId": "ai-dvisor-466322",
          "region": "us-west1"
        }
      },
      {
        "source": "/apps/**",
        "run": {
          "serviceId": "ai-dvisor-466322",
          "region": "us-west1"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}